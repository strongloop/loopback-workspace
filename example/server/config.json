{
  "restApiRoot": "/api",
  "host": "127.0.0.1",
  "httpport": 3003,
  "url": "localhost",
  "port": 443,
  "enableHttps": true,
  "httpsOnly": false,
  "remoting": {
    "context": {
      "enableHttpContext": false
    },
    "rest": {
      "normalizeHttpPath": false,
      "xml": false
    },
    "json": {
      "strict": false,
      "limit": "100kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "100kb"
    },
    "cors": false,
    "errorHandler": {
      "disableStackTrace": false
    }
  },
  "legacyExplorer": false
}
