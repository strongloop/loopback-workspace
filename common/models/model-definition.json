      "json": false
    },
    "facetName": {
      "type": "string",
      "required": true,
      "json": false
    },
    "readonly": {
      "type": "boolean",
      "json": false
    },
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "plural": "string",
    "base": "string",
    "strict": "boolean",
